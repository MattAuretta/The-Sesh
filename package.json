{
  "name": "the_sesh",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "auth0-js": "^9.8.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "http-proxy-middleware": "^0.19.1",
    "if-env": "^1.0.4",
    "jwks-rsa": "^1.3.0",
    "moment": "^2.23.0",
    "mongoose": "^5.4.0",
    "node-sass": "^4.11.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}